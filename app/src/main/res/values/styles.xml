<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="TitleStyle">
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:letterSpacing">-0.03</item>
        <item name="android:lineSpacingExtra">2sp</item>
        <item name="android:textSize">18sp</item>
        <item name="android:translationY">-1.11sp</item>
    </style>

    <style name="RatingTitleStyle">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:letterSpacing">-0.04</item>
        <item name="android:lineSpacingExtra">-5sp</item>
        <item name="android:textSize">24sp</item>
    </style>

    <style name="ProfileValuesTextStyle">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:lineSpacingExtra">1sp</item>
        <item name="android:textSize">14sp</item>
        <item name="android:translationY">-0.53sp</item>
    </style>

    <style name="ChooseIconText">
        <item name="android:textSize">14sp</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">@color/secondary_color</item>
        <item name="android:lineSpacingExtra">1sp</item>
        <item name="android:translationY">-0.53sp</item>
        <item name="android:fontFamily">@font/inter</item>
    </style>

    <style name="BorderedButtonStyle" parent="TextAppearance.AppCompat.Widget.Button">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">-1sp</item>
        <item name="android:fontFamily">@font/inter_medium</item>
        <item name="android:textColor">@color/on_primary_color</item>
        <item name="backgroundTint">@color/primary_color</item>
        <item name="cornerRadius">4dp</item>
        <item name="strokeColor">@color/secondary_color</item>
        <item name="strokeWidth">1dp</item>
    </style>

    <style name="ListLabelText">
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">@color/secondary_color</item>
        <item name="android:lineSpacingExtra">1sp</item>
        <item name="android:translationY">-0.53sp</item>
        <item name="android:fontFamily">@font/inter_medium</item>
    </style>

    <style name="PeopleTitleText">
        <item name="android:textSize">16sp</item>
        <item name="android:lineSpacingExtra">5sp</item>
        <item name="android:translationY">-2.32sp</item>
        <item name="android:fontFamily">@font/inter</item>
    </style>

    <style name="PeopleActionSubscribeText">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">1sp</item>
        <item name="android:translationY">-0.53sp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">@color/secondary_color</item>
    </style>

    <style name="PeopleActionUnsubscribeText">
        <item name="android:textSize">14sp</item>
        <item name="android:lineSpacingExtra">1sp</item>
        <item name="android:translationY">-0.53sp</item>
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:letterSpacing">-0.02</item>
        <item name="android:textColor">@color/hint_color</item>
    </style>

    <style name="TextInputLayoutTheme">
        <item name="colorControlActivated">@color/secondary_color</item>
    </style>

    <style name="TabTitle">
        <item name="android:fontFamily">@font/inter</item>
        <item name="android:textSize">14sp</item>
        <item name="android:textColor">#99FFFFFF</item>
        <item name="android:lineSpacingExtra">-1sp</item>
    </style>
</resources>